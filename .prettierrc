{
    "plugins": ["prettier-plugin-tailwindcss", "@trivago/prettier-plugin-sort-imports"],
    "printWidth": 120,
    "importOrder": ["^react$", "<THIRD_PARTY_MODULES>", "^[@/$]", "^[./$]"],
    "importOrderSeparation": true,
    "singleQuote": true,
    "JsxSingleQuote": true,
    "trailingComma": "none",
    "tabWidth": 4,
    "semi": false,
    "useTabs": false,
    "bracketSpacing": true
}
